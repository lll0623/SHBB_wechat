<view class="headerWrap clearfix rel bgF mb30">
    <image src="{{headerImg}}" class="fl mr30" />
    <view class="fl fs30 C0 inlineB userTel" bindtap="mineFastEnter" data-type="5">{{userTel}}</view>
    <!-- <view class="icon i-set fr" bindtap="mineFastEnter" data-type="4"></view> -->
</view>
<view class="bgF lists fs28 C0">
    <view class="clearfix comBottom" bindtap="mineFastEnter" data-type="1">
        <view class="fl">我的收藏</view>
        <view class="fr icon i-right rel"></view>
    </view>
    <view class="clearfix comBottom" bindtap="mineFastEnter" data-type="2">
        <view class="fl">我的店铺</view>
        <view class="fr icon i-right rel"></view>
    </view>
    <view class="clearfix" bindtap="mineFastEnter" data-type="3">
        <view class="fl">我的需求</view>
        <view class="fr icon i-right rel"></view>
    </view>
</view>
<view class="clearfix tc mt30 fs28 C0 bgF p20" bindtap="logoutFunc" wx:if="{{isLogin}}">
    退出登录
</view>
